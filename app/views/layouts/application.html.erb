<!DOCTYPE html>
<html>
<head>
  <title><%= page_title_for(yield(:title)) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= yield(:head) %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
